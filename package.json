{
  "name": "quollabore-cypress-reporter",
  "version": "0.1.1",
  "description": "Cypress Node events reporter for Quollabore portal",
  "type": "module",
  "main": "dist/plugin.cjs",
  "module": "dist/plugin.js",
  "types": "dist/plugin.d.ts",
  "exports": {
    ".": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/plugin.ts --dts --format esm,cjs --out-dir dist --clean"
  },
  "peerDependencies": {
    "cypress": ">=10.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.5.0"
  }
}
